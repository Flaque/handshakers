{
  "env": {
    "development": {
      "presets": "next/babel",
      "plugins": [
        [
          "styled-components",
          { "ssr": true, "displayName": true, "preprocess": false }
        ],
        ["transform-class-properties"]
      ]
    },
    "production": {
      "presets": "next/babel",
      "plugins": [
        [
          "styled-components",
          { "ssr": true, "displayName": true, "preprocess": false }
        ],
        ["transform-class-properties"]
      ]
    },
    "test": {
      "presets": [["env", { "modules": "commonjs" }], "next/babel"],
      "plugins": [
        [
          "styled-components",
          { "ssr": true, "displayName": true, "preprocess": false }
        ],
        ["transform-class-properties"]
      ]
    }
  }
}
